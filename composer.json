    "keywords": ["toporia", "socialite", "oauth", "authentication", "social-login", "google", "facebook", "github"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Phungtruong7820",
            "email": "minhphung485@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "toporia/framework": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Toporia\\Socialite\\": "src/"
        },
        "files": [
